<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="w.com.wk.freelife">
    <!--android:theme="@style/AppTheme"-->
    <application
        android:name=".FreeLifeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <uses-permission android:name="android.permission.INTERNET"/>
        <uses-permission android:name="android.permission.VIBRATE"/>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.hold.NewListActivtiy"/>
        <activity android:name=".ui.hold.ListDetailActivtiy"/>
        <activity android:name=".ui.widget.WtInterestActivtiy"/>
        <activity android:name=".ui.widget.WtTestActivtiy"/>
        <activity android:name=".ui.widget.WtMuseActivtiy"/>
        <activity android:name=".ui.widget.WtExerciseActivtiy"/>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
    </application>


</manifest>